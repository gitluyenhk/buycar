Dhcp Server \\PHUQUOC.cybersoft.vn Scope 172.18.5.64 set optionvalue 44 IPADDRESS "172.17.1.21" "172.17.1.20" 
Dhcp Server \\PHUQUOC.cybersoft.vn Scope 172.18.5.64 set optionvalue 46 BYTE "8" 
Dhcp Server \\PHUQUOC.cybersoft.vn Scope 172.18.5.64 set optionvalue 252 STRING "http://donkey.cybersoft.vn:8083/proxy.pac" 
Dhcp Server \\PHUQUOC.cybersoft.vn Scope 172.18.5.64 set optionvalue 121 BINARY "" 

